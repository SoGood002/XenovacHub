local ui = game:GetService("CoreGui"):FindFirstChild("FluxLib")
if ui then
    ui:Destroy()
end
local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()

local win = Flux:Window("Xenovac Hub", "Prison Life", Color3.fromRGB(255, 110, 48), Enum.KeyCode.LeftControl)
local tab = win:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")

tab:Label("This is a Gun")
tab:Line()

tab:Button("ShotGun", "This function let you get Remington 870.", function()
Flux:Notification("Get Remington 870  Successfully!", "Alright")
wait()
local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
local Event = game:GetService("Workspace").Remote.ItemHandler
Event:InvokeServer(A_1)
end)

tab:Button("AK-47", "This function let you get AK-47.", function()
Flux:Notification("Get AK-47 Successfully!", "Alright")
wait()
local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["AK-47"].ITEMPICKUP
local Event = game:GetService("Workspace").Remote.ItemHandler
Event:InvokeServer(A_1)
end)

tab:Button("M9", "This function let you get M9.", function()
Flux:Notification("Get M9 Successfully!", "Alright")
wait()
local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["M9"].ITEMPICKUP
local Event = game:GetService("Workspace").Remote.ItemHandler
Event:InvokeServer(A_1)
end)

tab:Line()

tab:Label("Speed and Jump")
tab:Line()
tab:Slider("Walkspeed", "Makes your faster.", 16, 100,1,function(v)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
print(t)
end)
tab:Slider("Jumppower", "Makes your jump higher.", 49, 100,1,function(v)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
print(t)
end)

tab:Line()

local tab = win:Tab("Credit", "http://www.roblox.com/asset/?id=6023426915")
tab:Label("Pass Left Ctrl To Open/Close Gui")
tab:Label("Discord : discord.gg/96NtVTkVER")
tab:Label("Credit : Xenovac Exploit")
